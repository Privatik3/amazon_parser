<script type="text/javascript">

maintainHeight = function(){
    var mainHolder = document.getElementById("main-image-container");
    var imgTagWrapperId = document.getElementById("imgTagWrapperId");
    if(mainHolder && typeof mainHolder != 'undefined'){
        var ratio = 1.0;
        var shouldAutoPlay = false;
        var naturalMainImageSize = false;
        var videoSizes = [[355, 219], [450, 277], [425, 262], [466, 287], [522, 322]];
        
        	var rightMargin = 108;
        	mainHolder.style.marginRight = rightMargin + 'px';
        
        
	    var mainImgHeightPartitions = [500, 768];
	    var mainImgMaxSizes = [[250, 250], [350, 350], [500, 500]];
	    var mainImgMaxHeight;
	    var mainImgMaxWidth;
            var partition = 0;
            var viewPortHeight = window.innerHeight || document.documentElement.clientHeight || document.body.clientHeight;
            for(var i=0;i<mainImgHeightPartitions.length;i++){
                if(viewPortHeight>mainImgHeightPartitions[i]){
                    partition = i+1;
                }
              }
              if(mainImgMaxSizes.length>=partition){
                 mainImgMaxHeight = mainImgMaxSizes[partition][1];
                 mainImgMaxWidth = mainImgMaxSizes[partition][0];
              }
        
        var width = mainHolder.offsetWidth;
        var containerHeight = width/ratio;
        containerHeight = Math.min(containerHeight, 700);
        var aspectRatio = 522/322

        var landingImage = document.getElementById("landingImage");

        var imageHeight = containerHeight;
        var imageWidth = width;

        if(!shouldAutoPlay) {
            imageHeight = Math.min(imageHeight, 322);
            imageWidth = Math.min(imageWidth, 522);
        }

        
            imageHeight = Math.min(imageHeight, mainImgMaxHeight);
            imageWidth = Math.min(imageWidth, mainImgMaxWidth);
        
         var imageWidthBasedOnHeight = imageHeight * aspectRatio;
         var imageHeightBasedOnWidth = imageWidth / aspectRatio;

         imageHeight = Math.min(imageHeight, imageHeightBasedOnWidth);
         imageWidth = Math.min(imageWidth, imageWidthBasedOnHeight);

        if(typeof mainImgMaxHeight !== 'undefined' && mainImgMaxHeight){ 
            containerHeight = Math.min(mainImgMaxHeight, containerHeight);
        }

        mainHolder.style.height = containerHeight + "px";
        if(imgTagWrapperId && typeof imgTagWrapperId !== 'undefined' ){
            imgTagWrapperId.style.height = containerHeight + "px";
        }
        if(landingImage && !naturalMainImageSize) {
            landingImage.style.maxHeight = imageHeight + "px";
            landingImage.style.maxWidth  = imageWidth + "px";
        }
        if(shouldAutoPlay){
            if(landingImage){
                landingImage.style.height = imageHeight + "px";
                landingImage.style.width  = imageWidth + "px";
            }
        }
    }
};
maintainHeight();

window.onresize = function(){
    maintainHeight();
};



P.when('A').register("ImageBlockATF", function(A){
    var data = {
                'colorImages': { 'initial': [{"hiRes":"https://images-na.ssl-images-amazon.com/images/I/71JzvAp3p9L._SL1500_.jpg","thumb":"https://images-na.ssl-images-amazon.com/images/I/31dc6JTKHsL._SS40_.jpg","large":"https://images-na.ssl-images-amazon.com/images/I/31dc6JTKHsL.jpg","main":{"https://images-na.ssl-images-amazon.com/images/I/71JzvAp3p9L._SX355_.jpg":[219,355],"https://images-na.ssl-images-amazon.com/images/I/71JzvAp3p9L._SX450_.jpg":[278,450],"https://images-na.ssl-images-amazon.com/images/I/71JzvAp3p9L._SX425_.jpg":[262,425],"https://images-na.ssl-images-amazon.com/images/I/71JzvAp3p9L._SX466_.jpg":[288,466],"https://images-na.ssl-images-amazon.com/images/I/71JzvAp3p9L._SX522_.jpg":[322,522]},"variant":"MAIN","lowRes":null}]},
                'colorToAsin': {'initial': {}},
                'heroImage': {'initial': []},
                'heroVideo': {'initial': []},
                'holderRatio': 1.0,
                'holderMaxHeight': 700,
                'useStretchyImageFix': true,
                'videoIngressATFSlateThumbURL': '',
                'tabbedImmersiveViewTreatment': 'C',
                'isEnhancedImageBlockSize' : false,
                'atfEnhancedHoverOverlay' : false,
                'aibExp3Layout' : 0,
                'aibRuleName' : '',
                };
    A.trigger('P.AboveTheFold'); // trigger ATF event.
    return data;
});
